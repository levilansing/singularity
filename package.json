{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "deploy": "bun run build && bunx wrangler pages deploy dist --project-name=singularity-countdown"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.6",
    "bun-plugin-tailwind": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@genwave/svgmaker-mcp": "^0.3.1",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "wrangler": "^4.65.0"
  }
}
